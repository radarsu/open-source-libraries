{
    "name": "templord",
    "version": "1.0.4",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "templord": "./bin/run"
    },
    "description": "Tool to generate code from ejs templates from a convention-based structure.",
    "dependencies": {
        "@oclif/core": "1.12.1",
        "@oclif/plugin-help": "5.1.12",
        "@oclif/plugin-plugins": "2.1.0",
        "@types/inquirer": "8.2.1",
        "ejs": "3.1.8",
        "fast-glob": "3.2.11",
        "flat-log": "0.0.0",
        "fs-extra": "10.1.0",
        "inquirer": "8.2.4",
        "options-defaults": "2.0.40",
        "ts-import": "4.0.0-beta.6",
        "typescript": "4.7.4",
        "tslib": "2.4.0"
    },
    "devDependencies": {
        "@radrat-internal/node-cli-plugin-package": "0.0.0",
        "@radrat/cli": "3.0.0-beta.6",
        "@types/ejs": "3.1.1",
        "@types/fs-extra": "9.0.13",
        "@types/jest": "28.1.6",
        "@types/node": "16.11.42",
        "jest": "28.1.3",
        "ts-jest": "28.0.7"
    },
    "license": "MIT",
    "files": [
        "dist"
    ],
    "oclif": {
        "bin": "templord",
        "dirname": "templord",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ],
        "topicSeparator": " "
    }
}
