{
    "name": "templord",
    "version": "1.0.4",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "bin": {
        "templord": "./bin/run.js"
    },
    "description": "Tool to generate code from ejs templates from a convention-based structure.",
    "dependencies": {
        "@oclif/core": "2.7.1",
        "@oclif/plugin-help": "5.2.8",
        "@oclif/plugin-plugins": "2.4.3",
        "@types/inquirer": "9.0.3",
        "ejs": "3.1.9",
        "fast-glob": "3.2.12",
        "flat-log": "workspace:0.0.3",
        "fs-extra": "11.1.1",
        "inquirer": "9.1.5",
        "options-defaults": "workspace:2.0.40",
        "ts-import": "workspace:4.0.0-beta.10",
        "tslib": "2.5.0",
        "typescript": "5.0.2"
    },
    "devDependencies": {
        "@radrat-internal/node-cli-plugin-package": "0.0.0",
        "@radrat/cli": "3.0.0-beta.9",
        "@types/ejs": "3.1.2",
        "@types/fs-extra": "11.0.1",
        "@types/jest": "29.5.0",
        "@types/node": "18.15.10",
        "jest": "29.5.0",
        "ts-jest": "29.0.5"
    },
    "license": "MIT",
    "files": [
        "dist"
    ],
    "oclif": {
        "bin": "templord",
        "dirname": "templord",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ],
        "topicSeparator": " "
    }
}