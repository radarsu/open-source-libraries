{
    "name": "shareman",
    "version": "0.0.0",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "shareman": "./bin/run"
    },
    "description": "Tool to generate code from ejs templates from a convention-based structure.",
    "dependencies": {
        "@oclif/core": "1.12.1",
        "@oclif/plugin-help": "5.1.12",
        "@oclif/plugin-plugins": "2.1.0",
        "typescript": "4.7.4",
        "tslib": "2.4.0"
    },
    "devDependencies": {
        "@radrat-internal/node-cli-plugin-package": "0.0.0",
        "@radrat/cli": "3.0.0-beta.6",
        "@types/jest": "28.1.6",
        "@types/node": "16.11.42",
        "jest": "28.1.3",
        "ts-jest": "28.0.7"
    },
    "license": "MIT",
    "files": [
        "dist"
    ],
    "oclif": {
        "bin": "shareman",
        "dirname": "shareman",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ],
        "topicSeparator": " "
    }
}
