{
    "name": "missioner",
    "version": "0.0.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "missioner": "./bin/run"
    },
    "scripts": {
        "build": "shx rm -rf dist && tsc -b",
        "lint": "eslint . --ext .ts --config .eslintrc",
        "postpack": "shx rm -f oclif.manifest.json",
        "prepack": "yarn build && oclif manifest && oclif readme",
        "version": "oclif readme && git add README.md"
    },
    "dependencies": {
        "@oclif/core": "1.11.0",
        "@oclif/plugin-help": "5.1.12",
        "@oclif/plugin-plugins": "2.1.0",
        "ejs": "3.1.8",
        "inquirer": "8.2.4",
        "nanoid": "3.3.4",
        "node-ssh": "12.0.4",
        "pkg": "5.8.0",
        "ssh2-streams": "0.4.10",
        "ts-import": "4.0.0-beta.6"
    },
    "devDependencies": {
        "@types/ejs": "3.1.1",
        "@types/inquirer": "8.2.1",
        "@types/node": "16.11.42",
        "@types/ssh2": "1.11.4",
        "@types/ssh2-streams": "0.1.9",
        "globby": "11.0.4",
        "oclif": "3.1.1",
        "shx": "0.3.4",
        "ts-node": "10.9.1",
        "tslib": "2.4.0",
        "typescript": "4.7.4"
    },
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "oclif": {
        "bin": "missioner",
        "dirname": "missioner",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ],
        "topicSeparator": " ",
        "topics": {
            "launch": {
                "description": "Launches selected missions on target hosts."
            },
            "rollback": {
                "description": "Say hello to the world and others"
            }
        }
    }
}
